{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": "warn",
        "no-plusplus": 0,
        "no-underscore-dangle": ["error"],
        "max-len": ["error", 120]
    }
}
